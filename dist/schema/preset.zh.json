{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "设置": {
      "type": "object",
      "properties": {
        "上下文长度": {
          "type": "number",
          "minimum": 0,
          "maximum": 2000000,
          "description": "最大上下文 token 数. 酒馆计算出的上下文 token 数虚高, 容易在上下文 token 数没有达到限制时就报错, 因此建议调到最大 2000000"
        },
        "最大回复token数": {
          "type": "number",
          "minimum": 0,
          "description": "最大回复 token 数"
        },
        "每次回复数": {
          "description": "每次生成几个回复",
          "default": 1,
          "$ref": "#/$defs/__schema0"
        },
        "流式传输": {
          "type": "boolean",
          "description": "是否流式传输"
        },
        "温度": {
          "type": "number",
          "minimum": 0,
          "maximum": 2,
          "description": "温度"
        },
        "频率惩罚": {
          "type": "number",
          "minimum": -2,
          "maximum": 2,
          "description": "频率惩罚"
        },
        "存在惩罚": {
          "type": "number",
          "minimum": -2,
          "maximum": 2,
          "description": "存在惩罚"
        },
        "top_p": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "重复惩罚": {
          "description": "重复惩罚",
          "default": 1,
          "$ref": "#/$defs/__schema1"
        },
        "min_p": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "top_k": {
          "type": "number",
          "minimum": 0,
          "maximum": 500
        },
        "top_a": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "种子": {
          "description": "种子, -1 表示随机",
          "default": -1,
          "$ref": "#/$defs/__schema2"
        },
        "压缩系统消息": {
          "type": "boolean",
          "description": "压缩系统消息: 将连续的系统消息合并为一条消息"
        },
        "推理强度": {
          "type": "string",
          "enum": [
            "自动",
            "最小",
            "低",
            "中",
            "高",
            "最大"
          ],
          "description": "推理强度, 即内置思维链的投入程度. 例如, 如果酒馆直连 gemini-2.5-flash, 则`最小`将会不使用内置思维链"
        },
        "请求思维链": {
          "description": "请求思维链: 允许模型返回内置思维链的思考过程; 注意这只影响内置思维链显不显示, 不决定模型是否使用内置思维链",
          "default": true,
          "$ref": "#/$defs/__schema3"
        },
        "请求图片": {
          "description": "请求图片: 允许模型在回复中返回图片",
          "default": true,
          "$ref": "#/$defs/__schema4"
        },
        "启用函数调用": {
          "description": "启用函数调用: 允许模型使用函数调用功能; 比如 cursor 借此在回复中读写文件、运行命令",
          "default": true,
          "$ref": "#/$defs/__schema5"
        },
        "启用网络搜索": {
          "description": "启用网络搜索: 允许模型使用网络搜索功能",
          "default": true,
          "$ref": "#/$defs/__schema6"
        },
        "允许发送图片": {
          "description": "是否允许发送图片作为提示词",
          "default": "自动",
          "$ref": "#/$defs/__schema7"
        },
        "允许发送视频": {
          "description": "是否允许发送视频作为提示词",
          "default": true,
          "$ref": "#/$defs/__schema8"
        },
        "角色名称前缀": {
          "description": "角色名称前缀: 是否要为消息添加角色名称前缀, 以及怎么添加\n- 无: 不添加\n- 默认: 为与角色卡不同名的消息添加角色名称前缀, 添加到 `content` 字段开头 (即发送的消息内容是 `角色名: 消息内容`)\n- 内容: 为所有消息添加角色名称前缀, 添加到 `content` 字段开头 (即发送的消息内容是 `角色名: 消息内容`)\n- 补全: 在发送给模型时, 将角色名称写入到 `name` 字段; 仅支持字母数字和下划线, 不适用于 Claude、Google 等模型",
          "default": "无",
          "$ref": "#/$defs/__schema9"
        },
        "用引号包裹用户消息": {
          "description": "用引号包裹用户消息: 在发送给模型之前, 将所有用户消息用引号包裹",
          "$ref": "#/$defs/__schema10"
        }
      },
      "required": [
        "上下文长度",
        "最大回复token数",
        "流式传输",
        "温度",
        "频率惩罚",
        "存在惩罚",
        "top_p",
        "压缩系统消息",
        "推理强度"
      ],
      "additionalProperties": false
    },
    "锚点": {
      "description": "用于存放 YAML 锚点, 不会被实际使用",
      "$ref": "#/$defs/__schema11"
    },
    "提示词": {
      "description": "提示词列表里已经添加的提示词",
      "$ref": "#/$defs/__schema38"
    },
    "未添加的提示词": {
      "description": "下拉框里的, 没有添加进提示词列表的提示词",
      "$ref": "#/$defs/__schema38"
    },
    "扩展字段": {
      "description": "扩展字段: 用于为预设绑定额外数据",
      "$ref": "#/$defs/__schema39"
    }
  },
  "required": [
    "设置",
    "提示词",
    "未添加的提示词"
  ],
  "additionalProperties": false,
